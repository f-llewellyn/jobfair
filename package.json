{
	"name": "jobfair",
	"version": "1.0.0",
	"description": "A job listing site designed for young workers in the uk",
	"main": "server.js",
	"scripts": {
		"server": "nodemon server.js --ignore 'client/'",
		"client": "npm run dev --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"start": "node server.js",
		"build": "npm install --prefix client && npm run build --prefix"
	},
	"author": "Finn Llewellyn",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"mongoose": "^6.1.0",
		"morgan": "^1.10.0"
	},
	"devDependencies": {
		"concurrently": "^7.1.0",
		"dotenv": "^16.0.0",
		"nodemon": "^2.0.15"
	}
}
